<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- ========================================================== -->
        <!-- Corporate Audit Log Sequence                               -->
        <!-- Auto-generates audit log reference numbers: AUDIT/2026/0001 -->
        <!-- ========================================================== -->
        
        <record id="seq_ops_corporate_audit_log" model="ir.sequence">
            <field name="name">Corporate Audit Log Sequence</field>
            <field name="code">ops.corporate.audit.log</field>
            <field name="prefix">AUDIT/%(year)s/</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>
        
    </data>
</odoo>
