<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- OPS Corporate PDF Layout: wkhtmltopdf header/footer for page numbers -->

    <template id="ops_corporate_pdf_layout">
        <!-- Empty header (prevents default wkhtmltopdf header, no visual output) -->
        <div class="header" style="border: none !important; padding: 0; margin: 0; max-height: 0; overflow: hidden; box-shadow: none !important;"/>

        <!-- Report content slot -->
        <t t-out="0"/>

        <!-- wkhtmltopdf footer: Badge + Page numbers -->
        <div class="footer" style="border: none !important; box-shadow: none !important; outline: none !important; padding: 0 12mm; margin: 0;">
            <span style="font-size: 7pt; color: #94a3b8; font-family: Arial, Helvetica, sans-serif;">Powered by</span>
            <img src="data:image/png;base64," style="height: 16px; vertical-align: middle; margin-left: 4px;" alt="OPS Framework"/>
            <span style="float: right; font-size: 8pt; color: #94a3b8; font-family: Arial, Helvetica, sans-serif;">Page <span class="page"/> of <span class="topage"/></span>
        </div>
    </template>
</odoo>
